generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model wishes {
  id         Int      @id @default(autoincrement())
  name       String
  limit_date DateTime @db.Timestamptz(6)
  status     status
}

enum status {
  canceled
  pending
  compliment
}
